{
  "bin": "dist/examples/DisplayWindow.js",
  "main": "dist/lib/index.js",
  "name": "litetgss",
  "version": "0.0.0-preview.4",
  "description": "LiteTGSS framework to build game using sfml.js in similar way as LiteRGSS2!",
  "keywords": [
    "LiteTGSS",
    "Game",
    "SFML"
  ],
  "author": "Nuri Yuri",
  "homepage": "https://github.com/NuriYuri/liteTGSS",
  "scripts": {
    "build": "tsc",
    "start": "ts-node examples/DisplayWindow.ts"
  },
  "dependencies": {
    "sfml.js": "^0.0.1-preview.42"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "assets/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "files": [
    "index.d.ts",
    "dist/lib/*.d.ts",
    "dist/lib/*.js",
    "dist/lib/*.js.map",
    "dist/lib/**/*.d.ts",
    "dist/lib/**/*.js",
    "dist/lib/**/*.js.map",
    "lib/**/*",
    "src/**/*",
    "tsconfig.json"
  ]
}
